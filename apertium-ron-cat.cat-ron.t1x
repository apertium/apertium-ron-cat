    <def-macro n="conj_verb2" npar="2" comment="Conjuga un verb compost doble">
        <choose>
          <when> 
            <test>
              <or>
              <and>
                <equal caseless="yes">
                  <clip pos="1" side="sl" part="lem"/>
                  <lit v="anar"/>
                </equal>
                <equal>
                  <clip pos="2" side="sl" part="a_temps"/>
                  <lit-tag v="inf"/>
                </equal>
              </and>
              <and>
                <equal>
                  <clip pos="1" side="sl" part="a_verb"/>
                  <lit-tag v="vbhaver"/>
                </equal>
                <equal>
                  <clip pos="1" side="sl" part="a_temps"/>
                  <lit-tag v="pri"/>
                </equal>
                <equal>
                  <clip pos="2" side="sl" part="a_temps"/>
                  <lit-tag v="pp"/>
                </equal>
              </and>
              </or>
            </test>
            <let>
